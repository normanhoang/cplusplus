// swapping 2 values using pointers

#include <stdio.h>

void swap(int *i, int *j) {
	int temp = *i;
	*i = *j;
	*j = temp;
}

int main() {
	int i, j;
	printf("Please enter a number for i: ");
	scanf("%d", &i);
	printf("Please enter a number for j: ");
	scanf("%d", &j);
	swap(&i, &j);
	printf("%d %d\n", i, j);
	return 0;
}
